{
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "index.md",
          "**.md",
          "*.md"
        ],
        "exclude": [
          "_site/**"
        ]
      },
      {
        "files":[
          "recipes/toc.yml",
          "recipes/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appFooter": "Â© 2021 Ryhon",
      "_enableSearch": "true"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": ["ExtractSearchIndex", "CustomMemberIndexer"],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}